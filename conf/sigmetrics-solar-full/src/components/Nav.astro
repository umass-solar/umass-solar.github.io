---
import type { NavItem } from "../content/site";
const { items } = Astro.props as { items: NavItem[] };
---
<nav aria-label="Primary" style="display:flex; flex-wrap:wrap; gap: 10px; justify-content:flex-end;">
  {items.map((it) => (
    <a href={it.href} class="pill" style="white-space:nowrap;">
      {it.label}
    </a>
  ))}
</nav>
